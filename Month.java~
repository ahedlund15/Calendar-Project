import java.util.*;

public class Month
{
  private int mydays; //number of days in a month
  private int mynumb; //the number of the month (ie. 1 = january, etc.)
  private int myyear; //the year
  public Month(int number, int year, int days)
  {
    mydays = days;
    mynumb = number;
    myyear = year;
  }
}